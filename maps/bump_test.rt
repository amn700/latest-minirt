# Bump Map Test Scene
A 0.3 255,255,255
C 0,5,-12 0,-0.1,1 70
L 5,10,-5 0.9 255,255,255

# Floor plane with checkerboard pattern
pl 0,0,0 0,1,0 150,150,150 pat:checkers pa:50:50:50 pb:200:200:200

# Sphere with rocks texture and bump map (same texture as bump)
sp -4,2,0 3.0 180,180,180 texture:../z_textures/rocks.png bump:../z_textures/rocks.png bump_strength:0.8

# Sphere with brick texture and bump map
sp 4,2,0 3.0 200,200,200 texture:../z_textures/Texturelabs_Brick_120thumbnail.png bump:../z_textures/Texturelabs_Brick_120thumbnail.png bump_strength:0.6

# Center sphere without bump for comparison
sp 0,2,5 3.0 200,200,200 texture:../z_textures/rocks.png
